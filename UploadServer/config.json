{
    "api": {
        "port": 8000
    },
    "cloud": {
        "provider": "debug"
    },
    "aws": {
        "region":           "us-east-2",
        "upload_bucket":    "unhjhc-wibl-incoming",
        "sns_topic":        "arn:aws:sns:us-east-2:ACCOUNT_NUMBER:unhjhc-wibl-conversion"
    },
    "db": {
        "connection":   "/var/local/wibl/upload-server/db/loggers.db"
    },
    "cert": {
      "cert_file": "/var/local/wibl/upload-server/etc/certs/server.crt",
      "key_file": "/var/local/wibl/upload-server/etc/certs/server.key"
    },
    "logging": {
      "level": "debug"
    }
}
